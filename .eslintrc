{
    "env": {
        "browser": true,
        "jquery": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "indent": "off",
        "comma-spacing": ["error", { "before": false, "after": true }],
        "prettier/prettier": ["error", { "endOfLine": "auto" }]
    }
}
